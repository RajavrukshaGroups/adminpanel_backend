{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://admin-rajavruksha.vercel.app/$1",
      "permanent": true
    }
  ]
}
